<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Eduard_Sergiu_Android.BreakfastEntryPage"
             Title="Restaurants Page">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Add Restaurant"
 Clicked="OnBreakfastAddedClicked" />
    </ContentPage.ToolbarItems>
    <ListView x:Name="planView"
 Margin="20"
 ItemSelected="OnListViewItemSelected">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding BreakfastName}"
 Detail="{Binding Adress}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>