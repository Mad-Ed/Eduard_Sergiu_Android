<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Eduard_Sergiu_Android.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Eduard_Sergiu_Android"
    Shell.FlyoutBehavior="Disabled">

   
    <TabBar>
        <ShellContent
 Title="Welcome"
 ContentTemplate="{DataTemplate local:MainPage}"
 />
        <ShellContent
 Title="My BreakfastPlans"
 ContentTemplate="{DataTemplate local:PlanEntryPage}"
 />
        <ShellContent
 Title="My Restaurant"
 ContentTemplate="{DataTemplate local:BreakfastEntryPage}"
 />
        <ShellContent
 Title="My Favourite Breakfast Foods"
 ContentTemplate="{DataTemplate local:AboutPage}"
 />
    </TabBar>
</Shell>
